var maxProfit = function(prices) {
    let min=prices[0];
    let maxPro=0;
    for(let i=1;i<prices.length;i++){
        if(prices[i]<min){
            min=prices[i];
            }else{
                let profit = prices[i] - min;
                if(profit>maxPro){
                    maxPro=profit;
        }

        
        

        }
    }

    return maxPro;
    
};
console.log(maxProfit([7,1,5,3,6,4]));
